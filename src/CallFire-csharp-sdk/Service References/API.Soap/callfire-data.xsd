          </xs:appinfo>
          <documentation>
            <ann:summary>Unique ID of Contact</ann:summary>
            <ann:demoValue>123</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="firstName" type="string">
        <annotation>
          <documentation>
            <ann:summary>First name</ann:summary>
            <ann:demoValue>John</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="lastName" type="string">
        <annotation>
          <documentation>
            <ann:summary>Last name</ann:summary>
            <ann:demoValue>Doe</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="zipcode" type="string">
        <annotation>
          <documentation>
            <ann:summary>5 digit zipcode</ann:summary>
            <ann:demoValue>98154</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="homePhone" type="tns:PhoneNumber">
        <annotation>
          <documentation>
            <ann:summary>E.164 11 digit number</ann:summary>
            <ann:demoValue>18185551212</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="workPhone" type="tns:PhoneNumber">
        <annotation>
          <documentation>
            <ann:summary>E.164 11 digit number</ann:summary>
            <ann:demoValue>18185551313</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="mobilePhone" type="tns:PhoneNumber">
        <annotation>
          <documentation>
            <ann:summary>E.164 11 digit number</ann:summary>
            <ann:demoValue>18185551414</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="externalId" type="string">
        <annotation>
          <documentation>
            <ann:summary>id of contact defined by external system (NATION_BUILDER, GOOGLE_GROUPS, etc...)</ann:summary>
            <ann:demoValue>nb-1010</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <attribute name="externalSystem" type="string">
        <annotation>
          <documentation>
            <ann:summary>System where externalId was generated from (NATION_BUILDER, GOOGLE_GROUPS, etc...)</ann:summary>
            <ann:demoValue>NATION_BUILDER</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
      <anyAttribute processContents="lax" />
    </complexType>
  </element>
  <element name="ContactHistory">
    <annotation>
      <documentation>
        <ann:summary>List Calls or Texts associated with Contact</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <choice minOccurs="0" maxOccurs="unbounded">
        <element ref="tns:Call" />
        <element ref="tns:Text" />
      </choice>
    </complexType>
  </element>
  <element name="ContactList">
    <annotation>
      <documentation>
        <ann:summary>List of Contacts</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element name="Name" type="string" />
        <element name="Size" type="integer" />
        <element name="Created" type="dateTime" />
        <element name="Status" type="tns:ContactListStatus" />
      </sequence>
      <attribute name="id" type="long" use="required" />
    </complexType>
  </element>
  <element name="Region">
    <annotation>
      <documentation>
        <ann:summary>Region of number represented by city, state, prefix, etc...</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element minOccurs="0" name="Prefix" type="string">
          <annotation>
            <documentation>
              <ann:summary>4-7 digit prefix</ann:summary>
              <ann:demoValue>1920659</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="City" type="string">
          <annotation>
            <documentation>
              <ann:summary>Name of a city</ann:summary>
              <ann:demoValue>Seattle</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="State" type="string">
          <annotation>
            <documentation>
              <ann:summary>State abbreviation</ann:summary>
              <ann:demoValue>WA</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="Zipcode" type="string">
          <annotation>
            <documentation>
              <ann:summary>5 digit zipcode</ann:summary>
              <ann:demoValue>98154</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="Country" type="string">
          <annotation>
            <documentation>
              <ann:summary>2 digit country code</ann:summary>
              <ann:demoValue>US</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="Lata" type="string">
          <annotation>
            <documentation>
              <ann:summary>Lata</ann:summary>
              <ann:demoValue>674</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="RateCenter" type="string">
          <annotation>
            <documentation>
              <ann:demoValue>SEATTLE</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="Latitude" type="float">
          <annotation>
            <documentation>
              <ann:summary>Latitude</ann:summary>
              <ann:demoValue>47.61</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="Longitude" type="float">
          <annotation>
            <documentation>
              <ann:summary>Longitude</ann:summary>
              <ann:demoValue>-122.331</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="TimeZone" type="string">
          <annotation>
            <documentation>
              <ann:demoValue>America/Los_Angeles</ann:demoValue>
            </documentation>
          </annotation>
        </element>
      </sequence>
    </complexType>
  </element>
  <element name="LeaseInfo">
    <annotation>
      <documentation>
        <ann:summary>Info about Lease</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element minOccurs="0" name="LeaseBegin" type="date">
          <annotation>
            <documentation>
              <ann:summary>Date lease began</ann:summary>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="LeaseEnd" type="date">
          <annotation>
            <documentation>
              <ann:summary>Date lease ends</ann:summary>
            </documentation>
          </annotation>
        </element>
        <element name="AutoRenew" type="boolean">
          <annotation>
            <documentation>
              <ann:summary>Is lease set to renew each month</ann:summary>
            </documentation>
          </annotation>
        </element>
      </sequence>
    </complexType>
  </element>
  <element name="Number">
    <annotation>
      <documentation>
        <ann:summary>Info about Number like Region</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element name="Number" type="tns:PhoneNumber">
          <annotation>
            <documentation>
              <ann:summary>11 digit E.164 number</ann:summary>
              <ann:demoValue>13105551212</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element name="NationalFormat" type="string">
          <annotation>
            <documentation>
              <ann:summary>Phone number formatted for local</ann:summary>
              <ann:demoValue>(310) 555-1212</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element name="TollFree" type="boolean">
          <annotation>
            <documentation>
              <ann:summary>Is toll free number</ann:summary>
              <ann:demoValue>false</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" ref="tns:Region" />
        <element minOccurs="0" name="Status" type="tns:NumberStatus" />
        <element minOccurs="0" ref="tns:LeaseInfo" />
        <element minOccurs="0" ref="tns:NumberConfiguration" />
      </sequence>
    </complexType>
  </element>
  <element name="NumberConfiguration">
    <annotation>
      <documentation>
        <ann:summary>Configure Call and Text features</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element minOccurs="0" name="CallFeature" type="tns:NumberFeature" />
        <element minOccurs="0" name="TextFeature" type="tns:NumberFeature" />
        <element minOccurs="0" name="InboundCallConfigurationType" type="tns:InboundType" />
        <element minOccurs="0" name="InboundCallConfiguration">
          <complexType>
            <choice>
              <element ref="tns:CallTrackingConfig">
                <annotation>
                  <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <jaxb:bindings node="ancestor::xs:annotation/..">
                      <ann:annotate ann:restCondition="InboundCallConfigurationType=TRACKING" />
                    </jaxb:bindings>
                  </xs:appinfo>
                  <documentation>
                    <ann:condition field="InboundCallConfigurationType" value="TRACKING" />
                  </documentation>
                </annotation>
              </element>
              <element ref="tns:IvrInboundConfig">
                <annotation>
                  <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <jaxb:bindings node="ancestor::xs:annotation/..">
                      <ann:annotate ann:restCondition="InboundCallConfigurationType=IVR" />
                    </jaxb:bindings>
                  </xs:appinfo>
                  <documentation>
                    <ann:condition field="InboundCallConfigurationType" value="IVR" />
                  </documentation>
                </annotation>
              </element>
            </choice>
          </complexType>
        </element>
      </sequence>
    </complexType>
  </element>
  <element name="Keyword">
    <annotation>
      <documentation>
        <ann:summary>Info on Keyword like status and lease info</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element name="ShortCode" type="tns:PhoneNumber" />
        <element name="Keyword" type="string" />
        <element minOccurs="0" name="Status" type="tns:NumberStatus" />
        <element minOccurs="0" ref="tns:LeaseInfo" />
      </sequence>
    </complexType>
  </element>
  <complexType name="NumberOrderItem">
    <sequence>
      <element name="Ordered" type="int" />
      <element name="UnitCost" type="float" />
      <element minOccurs="0" name="Fulfilled">
        <simpleType>
          <list itemType="string" />
        </simpleType>
      </element>
    </sequence>
  </complexType>
  <element name="NumberOrder">
    <annotation>
      <documentation>
        <ann:summary>Info on NumberOrder like id, status, and cost</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element name="Status" type="tns:OrderStatus" />
        <element name="Created" type="dateTime" />
        <element name="TotalCost" type="float" />
        <element minOccurs="0" name="LocalNumbers" type="tns:NumberOrderItem" />
        <element minOccurs="0" name="TollFreeNumbers" type="tns:NumberOrderItem" />
        <element minOccurs="0" name="Keywords" type="tns:NumberOrderItem" />
      </sequence>
      <attribute name="id" type="long" use="required" />
    </complexType>
  </element>
  <element name="Label">
    <complexType>
      <sequence>
        <element name="Name" type="string" />
      </sequence>
    </complexType>
  </element>
</schema>