<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:tns="http://api.callfire.com/data" xmlns:ann="http://api.callfire.com/annotations" elementFormDefault="qualified" targetNamespace="http://api.callfire.com/data" jaxb:version="2.0" xmlns="http://www.w3.org/2001/XMLSchema">
  <import schemaLocation="callfire-ann.xsd" namespace="http://api.callfire.com/annotations" />
  <simpleType name="PhoneDigit">
    <restriction base="string">
      <pattern value="[0-9#\*]" />
    </restriction>
  </simpleType>
  <simpleType name="PhoneNumber">
    <restriction base="string">
      <pattern value="([a-zA-Z_]+)|(\+?[0-9]+)" />
    </restriction>
  </simpleType>
  <simpleType name="PhoneNumberList">
    <list itemType="tns:PhoneNumber" />
  </simpleType>
  <simpleType name="KeywordList">
    <list itemType="string" />
  </simpleType>
  <simpleType name="ActionState">
    <restriction base="string">
      <enumeration value="READY" />
      <enumeration value="SELECTED" />
      <enumeration value="FINISHED" />
      <enumeration value="DNC" />
      <enumeration value="DUP" />
      <enumeration value="INVALID" />
      <enumeration value="TIMEOUT" />
    </restriction>
  </simpleType>
  <simpleType name="Result">
    <restriction base="string">
      <enumeration value="LA" />
      <enumeration value="AM" />
      <enumeration value="BUSY" />
      <enumeration value="DNC" />
      <enumeration value="XFER" />
      <enumeration value="XFER_LEG" />
      <enumeration value="NO_ANS" />
      <enumeration value="UNDIALED" />
      <enumeration value="SENT" />
      <enumeration value="RECEIVED" />
      <enumeration value="DNT" />
      <enumeration value="TOO_BIG" />
      <enumeration value="INTERNAL_ERROR" />
      <enumeration value="CARRIER_ERROR" />
      <enumeration value="CARRIER_TEMP_ERROR" />
    </restriction>
  </simpleType>
  <simpleType name="RetryPhoneType">
    <restriction base="string">
      <enumeration value="FIRST_NUMBER" />
      <enumeration value="HOME_PHONE" />
      <enumeration value="WORK_PHONE" />
      <enumeration value="MOBILE_PHONE" />
    </restriction>
  </simpleType>
  <simpleType name="BroadcastType">
    <restriction base="string">
      <enumeration value="VOICE" />
      <enumeration value="IVR" />
      <enumeration value="TEXT" />
    </restriction>
  </simpleType>
  <simpleType name="InboundType">
    <restriction base="string">
      <enumeration value="TRACKING" />
      <enumeration value="IVR" />
    </restriction>
  </simpleType>
  <simpleType name="DayOfWeek">
    <restriction base="string">
      <enumeration value="SUNDAY" />
      <enumeration value="MONDAY" />
      <enumeration value="TUESDAY" />
      <enumeration value="WEDNESDAY" />
      <enumeration value="THURSDAY" />
      <enumeration value="FRIDAY" />
      <enumeration value="SATURDAY" />
    </restriction>
  </simpleType>
  <simpleType name="BroadcastStatus">
    <restriction base="string">
      <enumeration value="START_PENDING" />
      <enumeration value="RUNNING" />
      <enumeration value="STOPPED" />
      <enumeration value="FINISHED" />
      <enumeration value="ARCHIVED" />
    </restriction>
  </simpleType>
  <simpleType name="BatchStatus">
    <restriction base="string">
      <enumeration value="NEW" />
      <enumeration value="VALIDATING" />
      <enumeration value="ERRORS" />
      <enumeration value="SOURCE_ERROR" />
      <enumeration value="ACTIVE" />
    </restriction>
  </simpleType>
  <simpleType name="ContactListStatus">
    <restriction base="string">
      <enumeration value="ACTIVE" />
      <enumeration value="VALIDATING" />
      <enumeration value="ERRORS" />
      <enumeration value="DELETED" />
      <enumeration value="PARSE_FAILED" />
      <enumeration value="COLUMN_TOO_LARGE" />
    </restriction>
  </simpleType>
  <simpleType name="AnsweringMachineConfig">
    <restriction base="string">
      <enumeration value="AM_ONLY" />
      <enumeration value="AM_AND_LIVE" />
      <enumeration value="LIVE_WITH_AMD" />
      <enumeration value="LIVE_IMMEDIATE" />
    </restriction>
  </simpleType>
  <simpleType name="BigMessageStrategy">
    <restriction base="string">
      <enumeration value="SEND_MULTIPLE" />
      <enumeration value="DO_NOT_SEND" />
      <enumeration value="TRIM" />
    </restriction>
  </simpleType>
  <simpleType name="NotificationFormat">
    <restriction base="string">
      <enumeration value="XML" />
      <enumeration value="JSON" />
      <enumeration value="SOAP" />
      <enumeration value="EMAIL" />
    </restriction>
  </simpleType>
  <simpleType name="SubscriptionTriggerEvent">
    <restriction base="string">
      <enumeration value="UNDEFINED_EVENT" />
      <enumeration value="INBOUND_CALL_FINISHED" />
      <enumeration value="INBOUND_TEXT_FINISHED" />
      <enumeration value="OUTBOUND_CALL_FINISHED" />
      <enumeration value="OUTBOUND_TEXT_FINISHED" />
      <enumeration value="CAMPAIGN_STARTED" />
      <enumeration value="CAMPAIGN_STOPPED" />
      <enumeration value="CAMPAIGN_FINISHED" />
    </restriction>
  </simpleType>
  <simpleType name="NumberStatus">
    <restriction base="string">
      <enumeration value="PENDING" />
      <enumeration value="ACTIVE" />
      <enumeration value="RELEASED" />
      <enumeration value="UNAVAILABLE" />
    </restriction>
  </simpleType>
  <simpleType name="NumberFeature">
    <restriction base="string">
      <enumeration value="UNSUPPORTED" />
      <enumeration value="PENDING" />
      <enumeration value="DISABLED" />
      <enumeration value="ENABLED" />
    </restriction>
  </simpleType>
  <simpleType name="OrderStatus">
    <restriction base="string">
      <enumeration value="NEW" />
      <enumeration value="PROCESSING" />
      <enumeration value="FINISHED" />
      <enumeration value="ERRORED" />
      <enumeration value="VOID" />
      <enumeration value="WAIT_FOR_PAYMENT" />
      <enumeration value="ADJUSTED" />
    </restriction>
  </simpleType>
  <simpleType name="SoundStatus">
    <restriction base="string">
      <enumeration value="PENDING">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Sound is being created/processed, and is not ready for use.</ann:summary>
          </documentation>
        </annotation>
      </enumeration>
      <enumeration value="ACTIVE">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Sound is available for download/use.</ann:summary>
          </documentation>
        </annotation>
      </enumeration>
      <enumeration value="FAILED">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Sound creation failed and is not recoverable.</ann:summary>
          </documentation>
        </annotation>
      </enumeration>
      <enumeration value="ARCHIVED">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Sound has been archived and will be excluded from query results.</ann:summary>
          </documentation>
        </annotation>
      </enumeration>
    </restriction>
  </simpleType>
  <simpleType name="Voice">
    <restriction base="string">
      <pattern value="(MALE1)|(FEMALE1)|(FEMALE2)|(SPANISH1)" />
    </restriction>
  </simpleType>
  <complexType name="InboundConfig" abstract="true">
    <attribute name="id" type="long">
      <annotation>
        <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
          <jaxb:bindings node="ancestor::xs:annotation/.." xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
            <ann:annotate ann:excludeFromOperations="Update" xmlns:ann="http://api.callfire.com/annotations" />
          </jaxb:bindings>
        </xs:appinfo>
        <documentation>
          <ann:summary xmlns:ann="http://api.callfire.com/annotations">Unique ID of InboundConfig</ann:summary>
          <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">1</ann:demoValue>
        </documentation>
      </annotation>
    </attribute>
  </complexType>
  <complexType name="BroadcastConfig" abstract="true">
    <sequence>
      <element minOccurs="0" name="Created" type="dateTime">
        <annotation>
          <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
            <jaxb:bindings node="ancestor::xs:annotation/..">
              <ann:annotate ann:excludeFromOperations="Create" xmlns:ann="http://api.callfire.com/annotations" />
            </jaxb:bindings>
          </xs:appinfo>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">DateTime Broadcast was created 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">2012-10-26T21:32:52Z</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="FromNumber" type="tns:PhoneNumber">
        <annotation>
          <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
            <jaxb:bindings node="ancestor::xs:annotation/..">
              <ann:annotate ann:restParam="From" xmlns:ann="http://api.callfire.com/annotations" />
            </jaxb:bindings>
          </xs:appinfo>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">E.164 11 digit number or short code</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">13105551212</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" ref="tns:LocalTimeZoneRestriction" />
      <element minOccurs="0" name="RetryConfig">
        <annotation>
          <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
            <jaxb:bindings node="ancestor::xs:annotation/..">
              <ann:annotate ann:sinceVersion="1.1" xmlns:ann="http://api.callfire.com/annotations" />
            </jaxb:bindings>
          </xs:appinfo>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Retry logic for broadcast</ann:summary>
          </documentation>
        </annotation>
        <complexType>
          <sequence>
            <element minOccurs="0" default="1" name="MaxAttempts" type="int">
              <annotation>
                <documentation>
                  <ann:summary xmlns:ann="http://api.callfire.com/annotations">Max attempts to retry broadcast (default: 1)</ann:summary>
                  <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">1</ann:demoValue>
                </documentation>
              </annotation>
            </element>
            <element minOccurs="0" default="60" name="MinutesBetweenAttempts" type="int">
              <annotation>
                <documentation>
                  <ann:summary xmlns:ann="http://api.callfire.com/annotations">Minutes between broadcast attempts (default: 60)</ann:summary>
                  <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">60</ann:demoValue>
                </documentation>
              </annotation>
            </element>
            <element name="RetryResults">
              <annotation>
                <documentation>
                  <ann:summary xmlns:ann="http://api.callfire.com/annotations">Conditions to retry on</ann:summary>
                  <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">BUSY NO_ANS</ann:demoValue>
                </documentation>
              </annotation>
              <simpleType>
                <list itemType="tns:Result" />
              </simpleType>
            </element>
            <element minOccurs="0" name="RetryPhoneTypes">
              <annotation>
                <documentation>
                  <ann:summary xmlns:ann="http://api.callfire.com/annotations">Phone types to call in retry</ann:summary>
                  <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">FIRST_NUMBER HOME_PHONE</ann:demoValue>
                </documentation>
              </annotation>
              <simpleType>
                <list itemType="tns:RetryPhoneType" />
              </simpleType>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
    <attribute name="id" type="long">
      <annotation>
        <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
          <jaxb:bindings node="ancestor::xs:annotation/..">
            <ann:annotate ann:excludeFromOperations="Create,Send,Update" xmlns:ann="http://api.callfire.com/annotations" />
          </jaxb:bindings>
        </xs:appinfo>
        <documentation>
          <ann:summary xmlns:ann="http://api.callfire.com/annotations">Unique ID of BroadcastConfig</ann:summary>
          <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">1</ann:demoValue>
        </documentation>
      </annotation>
    </attribute>
  </complexType>
  <complexType name="ToNumber">
    <simpleContent>
      <extension base="tns:PhoneNumber">
        <attribute name="ClientData" type="string" />
        <anyAttribute processContents="lax" />
      </extension>
    </simpleContent>
  </complexType>
  <complexType name="ActionRecord" abstract="true">
    <sequence>
      <element name="Result" type="tns:Result" />
      <element name="FinishTime" type="dateTime">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">DateTime action finished in 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]' format</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">2012-10-26T21:32:52Z</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element name="BilledAmount" type="float">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Credits charged for call or text</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">1.00</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" name="QuestionResponse">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Question / Response pair defined in IVR campaign with &lt;a href="https://www.callfire.com/help/docs/callfire-xml/tags/stash"&gt;'stash'.&lt;/a&gt;</ann:summary>
          </documentation>
        </annotation>
        <complexType>
          <sequence>
            <element name="Question" type="string">
              <annotation>
                <documentation>
                  <ann:summary xmlns:ann="http://api.callfire.com/annotations">IVR stash 'varname'</ann:summary>
                  <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">question1</ann:demoValue>
                </documentation>
              </annotation>
            </element>
            <element name="Response" type="string">
              <annotation>
                <documentation>
                  <ann:summary xmlns:ann="http://api.callfire.com/annotations">IVR stash value associated with 'varname'</ann:summary>
                  <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">1</ann:demoValue>
                </documentation>
              </annotation>
            </element>
          </sequence>
        </complexType>
      </element>
    </sequence>
    <attribute name="id" type="long" use="required" />
  </complexType>
  <complexType name="Action">
    <sequence>
      <element name="FromNumber" type="tns:PhoneNumber">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">E.164 11 digit number or short code</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">67076</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element name="ToNumber" type="tns:ToNumber">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">List of E.164 11 digit numbers space seperated</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">13105551212 18185551212</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element name="State" type="tns:ActionState">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Current State of Action</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">FINISHED</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="BatchId" type="long">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Unique ID of Batch associated with Action</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">123</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="BroadcastId" type="long">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Unique ID of Broadcast associated with Action</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">123</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element name="ContactId" type="long">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Unique ID of Contact associated with Action</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">123</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element name="Inbound" type="boolean">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">Is inbound Action</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">false</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element name="Created" type="dateTime">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">DateTime Action was created 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">2012-10-26T21:32:52Z</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element name="Modified" type="dateTime">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations">DateTime Action was modified 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
            <ann:demoValue xmlns:ann="http://api.callfire.com/annotations">2012-10-26T21:32:52Z</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" name="FinalResult" type="tns:Result">
        <annotation>
          <documentation>
            <ann:summary xmlns:ann="http://api.callfire.com/annotations" />
            <ann:demoValue>NO_ANS</ann:demoValue>
          </documentation>
        </annotation>
      </element>
      <element minOccurs="0" maxOccurs="unbounded" ref="tns:Label" />
    </sequence>
    <attribute name="id" type="long" use="required">
      <annotation>
        <documentation>
          <ann:summary>Unique ID of action</ann:summary>
          <ann:demoValue>123</ann:demoValue>
        </documentation>
      </annotation>
    </attribute>
  </complexType>
  <element name="Broadcast">
    <annotation>
      <documentation>
        <ann:summary>A TEXT, IVR, or VOICE Broadcast</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <sequence>
        <element minOccurs="0" name="Name" type="string">
          <annotation>
            <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
              <jaxb:bindings node="ancestor::xs:annotation/..">
                <ann:annotate ann:requiredIn="POST" />
              </jaxb:bindings>
            </xs:appinfo>
            <documentation>
              <ann:summary>Name of Broadcast</ann:summary>
              <ann:demoValue>My Test Broadcast</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="Status" type="tns:BroadcastStatus">
          <annotation>
            <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
              <jaxb:bindings node="ancestor::xs:annotation/..">
                <ann:annotate ann:excludeFromOperations="Create" />
              </jaxb:bindings>
            </xs:appinfo>
            <documentation>
              <ann:summary>Status of Broadcast (response only)</ann:summary>
              <ann:demoValue>FINISHED</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="LastModified" type="dateTime">
          <annotation>
            <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
              <jaxb:bindings node="ancestor::xs:annotation/..">
                <ann:annotate ann:excludeFromOperations="Create" />
              </jaxb:bindings>
            </xs:appinfo>
            <documentation>
              <ann:summary>Last Modified 'CCYY-MM-DDThh:mm:ss[Z|(+|-)hh:mm]'</ann:summary>
              <ann:demoValue>2012-10-26T21:32:52Z</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <element minOccurs="0" name="Type" type="tns:BroadcastType">
          <annotation>
            <documentation>
              <ann:summary>Type of Broadcast</ann:summary>
              <ann:demoValue>TEXT</ann:demoValue>
            </documentation>
          </annotation>
        </element>
        <choice>
          <element ref="tns:VoiceBroadcastConfig">
            <annotation>
              <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <jaxb:bindings node="ancestor::xs:annotation/..">
                  <ann:annotate ann:restCondition="Type=VOICE" />
                </jaxb:bindings>
              </xs:appinfo>
              <documentation>
                <ann:summary>Configuration of a Voice Broadcast</ann:summary>
                <ann:condition field="Type" value="VOICE" />
              </documentation>
            </annotation>
          </element>
          <element ref="tns:TextBroadcastConfig">
            <annotation>
              <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <jaxb:bindings node="ancestor::xs:annotation/..">
                  <ann:annotate ann:restCondition="Type=TEXT" />
                </jaxb:bindings>
              </xs:appinfo>
              <documentation>
                <ann:summary>Configuration of a Text Broadcast</ann:summary>
                <ann:condition field="Type" value="TEXT" />
              </documentation>
            </annotation>
          </element>
          <element ref="tns:IvrBroadcastConfig">
            <annotation>
              <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <jaxb:bindings node="ancestor::xs:annotation/..">
                  <ann:annotate ann:restCondition="Type=IVR" />
                </jaxb:bindings>
              </xs:appinfo>
              <documentation>
                <ann:summary>Configuration of an IVR Broadcast</ann:summary>
                <ann:condition field="Type" value="IVR" />
              </documentation>
            </annotation>
          </element>
        </choice>
      </sequence>
      <attribute name="id" type="long">
        <annotation>
          <xs:appinfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xs="http://www.w3.org/2001/XMLSchema">
            <jaxb:bindings node="ancestor::xs:annotation/..">
              <ann:annotate ann:excludeFromOperations="Create" />
            </jaxb:bindings>
          </xs:appinfo>
          <documentation>
            <ann:summary>Unique ID of Broadcast</ann:summary>
            <ann:demoValue>1</ann:demoValue>
          </documentation>
        </annotation>
      </attribute>
    </complexType>
  </element>
  <element name="VoiceBroadcastConfig">
    <annotation>
      <documentation>
        <ann:summary>Configuration needed for a Voice Broadcast</ann:summary>
      </documentation>
    </annotation>
    <complexType>
      <complexContent mixed="false">
        <extension base="tns:BroadcastConfig">
          <sequence>
            <element minOccurs="0" name="AnsweringMachineConfig" type="tns:AnsweringMachineConfig">
              <annotation>
                <documentation>
                  <ann:summary>Action to take if machine answers</ann:summary>
                  <ann:demoValue>LIVE_IMMEDIATE</ann:demoValue>
                </documentation>
              </annotation>
            </element>
            <choice minOccurs="0">
              <element name="LiveSoundText" type="string" />
              <element name="LiveSoundId" type="long">
